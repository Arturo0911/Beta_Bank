<div class="container p-4">
    <form action="/main/filtro" method="POST" enctype="multipart/form-data">
        <div class="card">
            <div class="card-header">
                <h5 class="card-title">Record de cuenta</h5>
            </div>
            <div class="card-body">
                <h5 class="card-title">Seleccione la fecha para realizar la consulta</h5>
                <input class="form-control" type="date" name="record">
            </div>
        </div>
        <button class="btn btn-warning" id="Btn_submit" type="submit">Generar</button>
    </form>

</div>
<div class="container p-4">
    <form action="/main/imprimir" method="POST">
        <table class="table table-bordered">
        <thead>
            <tr>
                <th scope="col">NÂ° cuenta</th>
                <th scope="col">Fecha</th>
                <th scope="col">Detalle</th>
                <th scope="col">Valores</th>
            </tr>
        </thead>
        {{#each filtro}}
            <tbody>
                <tr>
                    <td>{{numero_cuenta}}</td>
                    <td>{{fecha}}</td>
                    <td>{{descripcion}}</td>
                    <td>{{valores}}</td>
                </tr>
            </tbody>
        {{/each}}
    </table>
    <button type="submit" class="btn btn-success">Imprimir</button>
    </form>
    
    
</div>